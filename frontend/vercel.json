{
  "version": 2,
  "buildCommand": "rm -rf node_modules package-lock.json && npm install && npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://mathii.pythonanywhere.com"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "https://mathii.pythonanywhere.com"
    }
  }
}